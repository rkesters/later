{
    "name": "@rkesters/later",
    "version": "1.0.0-1",
    "description": "Determine later (or previous) occurrences of recurring schedules",
    "keywords": [
        "schedule",
        "occurrences",
        "recur",
        "cron",
        "typescript"
    ],
    "author": "Robert Kesterson <rkesters@gmail.com>",
    "repository": {
        "type": "git",
