    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "devDependencies": {
        "@types/benchmark": "^1.0.33",
        "@types/chai": "^4.2.11",
        "@types/lodash": "^4.14.157",
        "@types/luxon": "^1.24.1",
        "@types/mocha": "^7.0.2",
        "@types/moment": "^2.13.0",
        "benchmark": "latest",
        "chai": "^4.2.0",
        "copyfiles": "^2.3.0",
        "del-cli": "^3.0.1",
        "mocha": "^8.0.1",
        "prettier": "^2.0.5",
        "should": ">=0.6.3",
        "ts-node": "^8.10.2",
        "typescript": "^3.9.5"
    },
